        jcr:description="Embed a PDF Viewer widget to display Document Cloud PDF's"
        jcr:primaryType="cq:PageContent"
        jcr:title="PDF Viewer"
        sling:resourceType="core-components-examples/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                cq:styleIds="[1545309110033]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <text_1740702241
                    cq:styleIds="[1644862132301]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;h1&gt;PDF Viewer&lt;sub&gt;v1&lt;/sub&gt;&lt;/h1&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <text
                    cq:styleIds="[1544762734201]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Embed PDF Viewer to display PDF file's stored in the DAM on the page.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <teaser_github_wcmio
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="wcm.io Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/github-logo.svg"
                    linkURL="https://github.com/wcm-io/wcm-io-wcm-core-components/tree/develop/bundles/core/src/main/webapp/app-root/components/pdfviewer/v1"/>
                <teaser_github_adobe
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Adobe Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/adobe-logo.svg"
                    linkURL="https://github.com/adobe/aem-core-wcm-components/tree/master/content/src/content/jcr_root/apps/core/wcm/components/pdfviewer/v1/pdfviewer"
                    textIsRich="true"/>
                <title_865328580
                    cq:styleIds="[1544759664489]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Examples"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h2"/>
                <title_290384408
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Full Window"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;The viewing area renders in the full browser. Best suited for storage and productivity applications.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo"
                    fullWidth="{Boolean}true">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <pdfviewer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core/wcm/components/pdfviewer/v1/pdfviewer"
                            borderless="false"
                            defaultViewMode="FIT_PAGE"
                            dockPageControls="true"
                            documentPath="/content/dam/core-components-examples/library/sample-assets/Bodea Brochure.pdf"
                            showAnnotationTools="false"
                            showDownloadPdf="true"
                            showFullScreen="true"
                            showLeftHandPanel="true"
                            showPageControls="true"
                            showPrintPdf="true"
                            type="FULL_WINDOW"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo>
                <title_290384402
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Sized Container"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506762
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;The sized container mode displays PDFs in a boxed container with landscape orientation. Best suited for presentations.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo"
                    fullWidth="{Boolean}true">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <pdfviewer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core/wcm/components/pdfviewer/v1/pdfviewer"
                            borderless="false"
                            defaultViewMode="FIT_PAGE"
                            dockPageControls="false"
                            documentPath="/content/dam/core-components-examples/library/sample-assets/Bodea Brochure.pdf"
                            showAnnotationTools="false"
                            showDownloadPdf="true"
                            showFullScreen="false"
                            showLeftHandPanel="true"
                            showPageControls="false"
                            showPrintPdf="true"
                            type="SIZED_CONTAINER"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_copy>
                <title_290384403
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="In-Line"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506763
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;All PDF pages rendered in line within a web page. Best suited for reading applications.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_copy_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo"
                    fullWidth="{Boolean}true">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <pdfviewer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core/wcm/components/pdfviewer/v1/pdfviewer"
                            borderless="false"
                            defaultViewMode="FIT_PAGE"
                            dockPageControls="true"
                            documentPath="/content/dam/core-components-examples/library/sample-assets/Bodea Brochure.pdf"
                            showAnnotationTools="false"
                            showDownloadPdf="true"
                            showFullScreen="true"
                            showLeftHandPanel="true"
                            showPageControls="true"
                            showPrintPdf="true"
                            type="IN_LINE"/>
                    </component>
