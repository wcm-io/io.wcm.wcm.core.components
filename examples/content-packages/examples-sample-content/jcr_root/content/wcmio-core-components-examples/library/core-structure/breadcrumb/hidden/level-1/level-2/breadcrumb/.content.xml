            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <text_1740702241
                    cq:styleIds="[1644862132301]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;h1&gt;Breadcrumb&lt;sub&gt;v3&lt;/sub&gt;&lt;/h1&gt;&#xA;"
                    textIsRich="true"/>
                <text
                    cq:styleIds="[1544762734201]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Breadcrumb allows navigation of the page hierarchy relative to the current page. It is commonly added to a page template, in the header or footer.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <teaser_github_wcmio
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="wcm.io Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/github-logo.svg"
                    linkURL="https://github.com/wcm-io/wcm-io-wcm-core-components/tree/develop/bundles/core/src/main/webapp/app-root/components/breadcrumb/v3"/>
                <teaser_github_adobe
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Adobe Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/adobe-logo.svg"
                    linkURL="https://github.com/adobe/aem-core-wcm-components/tree/master/content/src/content/jcr_root/apps/core/wcm/components/breadcrumb/v3/breadcrumb"
                    textIsRich="true"/>
                <title_865328580
                    cq:styleIds="[1544759664489]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Examples"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h2"/>
                <title_290384408
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Standard"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Simple breadcrumb with no configuration applied. Shows the current page in relation to the page hierarchy.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <demo
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/breadcrumb/v3/breadcrumb"
                            hideCurrent="false"
                            showHidden="false"
                            startLevel="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo>
                <title_290384408_copy_1530128142
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Navigation Start Level"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761_copy_1034941677
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;The &lt;i&gt;Navigation Start Level &lt;/i&gt;is set to two by default, which is typically the site root. In this example, we increase it to three to display a breadcrumb for a section one level lower.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_copy_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/breadcrumb/v3/breadcrumb"
                            hideCurrent="false"
                            showHidden="false"
                            startLevel="3"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_copy_copy>
                <title_290384408_copy
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Show Hidden Navigation Items"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761_copy_1888023046
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;By default, pages that are marked as &lt;i&gt;Hide in Navigation &lt;/i&gt; are not displayed in the breadcrumb. It is possible to configure a breadcrumb so that they are shown.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <demo_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/breadcrumb/v3/breadcrumb"
                            hideCurrent="false"
                            showHidden="true"
                            startLevel="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_copy>
                <Copy_x0020_of_x0020_title_290384408_copy
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Hide Current Page"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761_copy_1888023047
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;By default, the current page is&amp;nbsp;shown in the navigation. It is possible to configure a breadcrumb so that it is hidden.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <Copy_x0020_of_x0020_demo_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/breadcrumb/v3/breadcrumb"
                            hideCurrent="true"
                            showHidden="false"
                            startLevel="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
