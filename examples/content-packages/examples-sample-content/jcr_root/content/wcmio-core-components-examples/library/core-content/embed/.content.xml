            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <text_1740702241
                    cq:styleIds="[1644862132301]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;h1&gt;Embed&lt;sub&gt;v2&lt;/sub&gt;&lt;/h1&gt;&#xA;"
                    textIsRich="true"/>
                <text
                    cq:styleIds="[1544762734201]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Embed allows a third-party widget to be added to a page. Media can be added from a supported URL, a pre-configured embeddable or with safe freeform HTML.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <teaser_github_wcmio
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="wcm.io Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/github-logo.svg"
                    linkURL="https://github.com/wcm-io/wcm-io-wcm-core-components/tree/develop/bundles/core/src/main/webapp/app-root/components/embed/v2"/>
                <teaser_github_adobe
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Adobe Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/adobe-logo.svg"
                    linkURL="https://github.com/adobe/aem-core-wcm-components/tree/master/content/src/content/jcr_root/apps/core/wcm/components/embed/v2/embed"
                    textIsRich="true"/>
                <title_865328580
                    cq:styleIds="[1544759664489]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Examples"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h2"/>
                <title_290384408
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="URL"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;URLs to embeddable objects can be pasted in the edit dialog. Embed will check that the URL matches a supported processor and provide feedback to the author. Additional URL processors can be registered by a developer.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_1160408555
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo"
                    fullWidth="{Boolean}true">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <embed_616254787
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/embed/v2/embed"
                            type="url"
                            url="https://www.pinterest.com/pin/146859637829777606/"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
