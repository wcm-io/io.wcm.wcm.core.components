            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <text_1740702241
                    cq:styleIds="[1644862132301]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;h1&gt;Navigation&lt;sub&gt;v2&lt;/sub&gt;&lt;/h1&gt;&#xA;"
                    textIsRich="true"/>
                <text
                    cq:styleIds="[1544762734201]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Navigation allows an author to build a site navigation. Navigation structures can be built from live copies of a blueprint, language copies of a language master or from a simple page tree.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <teaser_github_wcmio
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="wcm.io Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/github-logo.svg"
                    linkURL="https://github.com/wcm-io/wcm-io-wcm-core-components/tree/develop/bundles/core/src/main/webapp/app-root/components/navigation/v2"/>
                <teaser_github_adobe
                    cq:styleIds="[1550165685463]"
                    jcr:description="&lt;p&gt;GitHub&lt;/p&gt;&#xD;&#xA;"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Adobe Documentation"
                    sling:resourceType="core-components-examples/components/teaser"
                    fileReference="/content/dam/core-components-examples/library/adobe-logo.svg"
                    linkURL="https://github.com/adobe/aem-core-wcm-components/tree/master/content/src/content/jcr_root/apps/core/wcm/components/navigation/v2/navigation"
                    textIsRich="true"/>
                <title_865328580
                    cq:styleIds="[1544759664489]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Examples"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h2"/>
                <title_290384408
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Standard"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337506761
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Simple navigation with no configuration applied. Shows a navigation structure built from a page tree. The navigation root is excluded by default.&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <demo
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/navigation/v2/navigation"
                            collectAllPages="true"
                            hideCurrent="false"
                            navigationRoot="/content/wcmio-core-components-examples"
                            showHidden="true"
                            skipNavigationRoot="true"
                            startLevel="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo>
                <title_290386545
                    cq:styleIds="[1544759676459]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Show Navigation Root"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_1337509867
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;By default, the navigation root is excluded in the navigation structure. In this example we display it.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_56474747
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/navigation/v2/navigation"
                            collectAllPages="true"
                            hideCurrent="false"
                            navigationRoot="/content/wcmio-core-components-examples"
                            showHidden="true"
                            skipNavigationRoot="false"
                            startLevel="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_56474747>
                <title_2061144506
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Skip Multiple Levels"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_130342336
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;Multiple root levels can be skipped. In this example we are skipping two, showing the main categories.&lt;/p&gt;&#xD;&#xA;"
                    textIsRich="true"/>
                <demo_1435920429
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/navigation/v2/navigation"
                            collectAllPages="true"
                            navigationRoot="/content/wcmio-core-components-examples"
                            structureStart="2"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io/wcm/core/components/tabs/v1/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_1435920429>
                <title
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Navigation Structure Depth"
                    sling:resourceType="wcm-io/wcm/core/components/title/v2/title"
                    type="h3"/>
                <text_894947512
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io/wcm/core/components/text/v2/text"
                    text="&lt;p&gt;By default, the navigation is built from all child pages. In this example, we again display the navigation root and reduce the structure depth to one. As a result, only one level from the navigation root is shown.&amp;nbsp;&lt;/p&gt;&#xA;"
                    textIsRich="true"/>
                <demo_650088868
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core-components-examples/components/demo">
                    <component
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/demo/component">
                        <navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm-io/wcm/core/components/navigation/v2/navigation"
                            collectAllPages="false"
                            hideCurrent="false"
                            navigationRoot="/content/wcmio-core-components-examples"
                            showHidden="true"
                            skipNavigationRoot="false"
                            startLevel="2"
                            structureDepth="1"/>
                    </component>
                    <info
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core-components-examples/components/tabs">
                        <properties
                            cq:panelTitle="Properties"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/properties"
                            reference="../../component"/>
                        <markup
                            cq:panelTitle="Markup"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/markup"
                            reference="../../component"/>
                        <json
                            cq:panelTitle="JSON"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="core-components-examples/components/demo/json"
                            reference="../../component"/>
                    </info>
                </demo_650088868>
